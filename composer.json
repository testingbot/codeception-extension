{
    "name": "testingbot/codeception-extension",
    "description": "Codeception extension to report meta-data back to TestingBot.",
    "prefer-stable": true,
    "license": "Apache-2.0",
    "type": "codeception-extension",
    "keywords": ["codeception", "testingbot", "selenium"],
    "authors": [
        {
            "name": "Jochen Delabie",
            "email": "info@testingbot.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "testingbot/testingbot-php": ">=1.0.0"
    },
    "require-dev": {
        "codeception/codeception": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Extension\\": "extensions/testingbot/"
        }
    }
}